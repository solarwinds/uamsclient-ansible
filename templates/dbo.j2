[
  {
    "agentID": "{{ uamsclientid }}",
    "dbAgents": [
      {% for dbAgent in dbAgents %}
      {
        "databaseType": "{{ dbAgent.databaseType }}",
        "name": "{{ dbAgent.name }}",
        "host": "{{ dbAgent.host }}",
        "port": "{{ dbAgent.port }}",
        "user": "{{ dbAgent.user }}",
        "password": "{{ dbAgent.password }}",
        "packetCaptureEnabled": {{ dbAgent.packetCaptureEnabled | default(false) | lower }},
        "metricsCaptureMethod": "{{ dbAgent.metricsCaptureMethod | default('') }}"
      }{% if not loop.last %},{% endif %}
      {% endfor %}
    ]
  }
]
